@using System.Globalization
@using Group11.Models
@model IEnumerable<Group11.Models.KhoahocIelts>
@{
    ViewBag.Title = "Ielts_kh";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/CSS/Ielts.css" rel="stylesheet" />


<div class="container">
    <div class="col-sm-6 col-md-6 col-sx-6">
        <img class="img-thumbnail img-responsive" style="width:500px;height:300px" src="data:image/jpeg;base64," />

    </div>

    <div class="col-sm-6 col-md-6">
        <h2> Giao Tiếp</h2>
        <hr style="height:5px; width:80px; margin-left:0px; border:none;background-color: blue;" />

        <p>
            Giao tiếp, viết tắt của Test of English for International Communication – Bài kiểm tra tiếng Anh giao tiếp quốc tế, là một chứng chỉ tiếng Anh quốc tế về giao tiếp dành cho người đi làm không phải là người sử dụng tiếng Anh làm tiếng mẹ đẻ, đặc biệt là
            những đối tượng muốn sử dụng tiếng Anh trong môi trường giao tiếp
        </p>
        <section>
            <h3>Cam kết khóa học</h3>
            <ul class="choose-list">
                <li> <i class="fas fa-check"> </i> .Mất gốc lấy lại căn bản, tạo cảm hứng mỗi khi đến lớp</li>
                <li> <i class="fas fa-check"> </i> .Tăng rõ khả năng giao tiếp sau 4 tháng học</li>
                <li> <i class="fas fa-check"> </i> .Cam kết môi trường học tập tốt nhất</li>
                <li> <i class="fas fa-check"> </i> .Học lại MIỄN PHÍ nếu học viên không đạt điểm đầu ra</li>
                <li> <i class="fas fa-check"> </i> .Có môi trường luyện tập hiệu quả</li>
            </ul>
        </section>
    </div>
</div>
<br />
<div class="container">
    <h2 class="text-center">Các Mốc Điểm</h2>
    <hr />

    @foreach (var item in Model)
    {
        <div class="col-sm-6 col-md-4 ">

            <div class="thumbnail">
                <img class="img-thumbnail img-responsive" src="@item.Picture" />
                <div class="caption">

                    <h3> @item.TenKH</h3>

                    <p> ► Giáo viên: @item.GiaoVien</p>

                    <p> ► Thời gian khóa học: @item.Thoigian</p>

                    <p> ► Nội dung: @item.Noidung</p>

                    <p> ► Giáo trình: @item.Giaotrinh</p>

                    <p> ► Hoạt động ngoại khóa: @item.hoatdong</p>

                    <p> ► Học phí: @item.hocphi </p>
                    <p> <a href="@Url.Action("Ielts_tl", "Home")"> ► Xem thời khóa biểu tại đây</a></p>
                </div>
            </div>

        </div>
    }

</div>


<div class="container">
    <h2>Đăng Ký Tư Vấn Miễn Phí</h2>
    <hr class="pull-left" style="height:5px; width:100px; border:none;background-color:yellow;" />
    <form>
        <div class="form-group" style="margin-top:60px">
            <input type="text" class="form-control" name="name" placeholder="Tên" value="">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" name="Email" placeholder="Email" value="">

        </div>
        <div class="form-group">
            <select id="mySelect1" class="form-control">
                <option value="450-550+" selected>Cho người mất gốc</option>
                <option value="650-700+">Cơ Bản</option>
                <option value="800+">Cho người đi làm</option>
            </select>
        </div>
        <div class="form-group">
            <textarea id="myTextarea2" class="form-control" placeholder="Lời Nhắn" rows="3"></textarea>
        </div>
        <p><button type="button" class="btn btn-primary" style="width:100px">Gửi</button></p>
    </form>
</div>




